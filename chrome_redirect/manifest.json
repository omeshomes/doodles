{
  "name": "Redirect",
  "description": "Redirect websites to google",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://facebook.com/*",
    "*://www.facebook.com/*",
    "webRequestBlocking"
  ]
}

